{% extends 'ZzChezZzortellBundle::layout.html.twig' %}

{% trans_default_domain 'ZzChezZzortellBundle' %}

{% block headers %}<meta name="viewport" content="width=552" />{% endblock %}

{% block stylesheets %}
    {{ parent () }}
    {% stylesheets 'bundles/zzchezzzortell/css/notebook.less' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block utils %}
    {#<script src="{{ asset('js/less.min.js') }}" type="text/javascript" ></script>#}
    {#<script src="{{ asset('js/jquery-1.11.2.js') }}" type="text/javascript" ></script>#}
{% endblock %}
{% block javascripts %}
    <script src="{{ asset('bundles/zzchezzzortell/js/dynamic.js') }}" type="text/javascript" ></script>
{% endblock %}


{% block body %}
{% autoescape false %}
{% spaceless %}
    {{ include ('ZzChezZzortellBundle::header.html.twig') }}
    <section id="notebook" style="vertical-align: middle;" >
        <section class="in_work" >
            <div class="rotation" >
                <i class="fa fa-connectdevelop fa-5x"></i>
            </div>
            <p>{{ 'renov.title' |trans }}</p>
        </section>
    {{ include ('ZzChezZzortellBundle::footer.html.twig') }}
    </section>
{% endspaceless %}
{% endautoescape %}
{% endblock %}
